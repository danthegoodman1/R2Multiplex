{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "r2-multiplex",
	"main": "src/index.ts",
	"compatibility_date": "2025-06-20",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
    {
      "binding": "AAAA",
      "bucket_name": "aaaa"
    },
    {
      "binding": "BBBB",
      "bucket_name": "bbbb"
    }
  ],
