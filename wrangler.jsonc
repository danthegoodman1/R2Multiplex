		"R2_KEY": "replaceme",
		"R2_SECRET": "replaceme",
		"ACCOUNT_ID": "replaceme",
		"CLIENT_ACCESS_KEY": "accesskey",
		"CLIENT_SECRET_KEY": "secretkey"
	},
